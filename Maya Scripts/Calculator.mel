
global proc float Add(float $nums[])
{
    int $len = size($nums);
    float $sum = $nums[0];
    for ($i = 1; $i < $len; $i++)
    {
        $sum += $nums[$i];
    }
    return $sum;
}

Add({10,1,1});

global proc float Diff(float $nums[])
{
    int $len = size($nums);
    float $diff = $nums[0];
    for ($i = 1; $i < $len; $i++)
    {
        $diff -= $nums[$i];
    }
    return $diff;
}

Diff({-10,5});

global proc float Mult(float $nums[])
{
    int $len = size($nums);
    float $pro = $nums[0];
    for ($i = 1; $i < $len; $i++)
    {
        $pro *= $nums[$i];
    }
    return $pro;
}

